{
  "root": true,
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
  "env": {
    "browser": true
  },
  "globals": {
    "gtag": true
  },
  "rules": {
    "no-unused-expressions": ["error", { "allowTaggedTemplates": true }],
    "import/prefer-default-export": "off",
    "react/destructuring-assignment": "off"
  }
}
